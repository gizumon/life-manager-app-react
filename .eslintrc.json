{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "error",
        "max-len": ["warn", { "code": 120 }]
    }
}
